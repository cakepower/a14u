{
  "name": "aesthetic-intelligence-lab-blog-landing-page",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "node --max-old-space-size=1536 ./server.js",
    "dev": "vite",
    "build": "NODE_OPTIONS='--max-old-space-size=4096' vite build",
    "preview": "vite preview",
    "build:css": "tailwindcss -i ./src/styles.css -o ./dist/styles.css --minify",
